{
  "userSecretsId": "aspnet-WebApplication-0799fe3e-6eaf-4c5f-b40e-7c6bfd5dfa9a",

  "dependencies": {
    "Filehook.AspNetCore.Http": { "target": "project" },
    "Filehook.Core": { "target": "project" },
    "Filehook.Proccessors.Image.ImageSharpProccessor": { "target": "project" },
    "Filehook.Storages.FileSystem": { "target": "project" },
    "Microsoft.AspNetCore.Diagnostics": "1.1.*",
    "Microsoft.AspNetCore.Mvc": "1.1.*",
    "Microsoft.AspNetCore.Razor.Tools": {
      "version": "1.1.0-preview4-final",
      "type": "build"
    },
    "Microsoft.AspNetCore.Server.IISIntegration": "1.1.*",
    "Microsoft.AspNetCore.Server.Kestrel": "1.1.*",
    "Microsoft.AspNetCore.StaticFiles": "1.1.*",
    "Microsoft.Extensions.Configuration.Json": "1.1.*",
    "Microsoft.Extensions.Logging": "1.1.*",
    "Microsoft.Extensions.Logging.Console": "1.1.*",
    "Microsoft.Extensions.Logging.Debug": "1.1.*"
  },

  "tools": {
    "Microsoft.AspNetCore.Razor.Tools": "1.1.0-preview4-final",
    "Microsoft.AspNetCore.Server.IISIntegration.Tools": "1.1.0-preview4-final"
  },

  "frameworks": {
    "netcoreapp1.1": {
      "dependencies": {
        "Microsoft.NETCore.App": {
          "version": "1.1.0",
          "type": "platform"
        }
      }
    }
  },

  "buildOptions": {
    "debugType": "portable",
    "emitEntryPoint": true,
    "preserveCompilationContext": true
  },

  "runtimeOptions": {
    "configProperties": {
      "System.GC.Server": true
    }
  },

  "publishOptions": {
    "include": [
      "wwwroot",
      "Views",
      "appsettings.json",
      "web.config"
    ]
  },

  "scripts": {
    "prepublish": [ "npm install", "bower install", "gulp clean", "gulp min" ],
    "postpublish": [ "dotnet publish-iis --publish-folder %publish:OutputPath% --framework %publish:FullTargetFramework%" ]
  },

  "tooling": {
    "defaultNamespace": "Filehook.Samples.AspNetCoreMvc"
  }
}
